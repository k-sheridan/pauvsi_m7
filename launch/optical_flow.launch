<launch>


  <node name="bottomCamera" pkg="PAUVSI-M7" type="cameraDriver" output="screen">
  	<param name="frame_rate" value="100" />
  	<param name="serial_number" value="15374828" />
  	<param name="camera_type" value="2" />
  	<param name="camera_position" value="bottomCam" />
  </node>
  
  <node pkg="svo_ros" type="vo" name="svo" clear_params="true" output="screen">
    
        <!-- Camera topic to subscribe to -->
        <param name="cam_topic" value="/bottomCam/color_distort" type="str" />
        
        <!-- Camera calibration file -->
        <rosparam file="$(find svo_ros)/param/camera_atan.yaml" />
        
        <!-- Default parameter settings: choose between vo_fast and vo_accurate -->
        <rosparam file="$(find svo_ros)/param/vo_fast.yaml" />

    </node>
  
</launch>